    local function tool() 
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then 
            return game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") 
        end 
    end 

    local function ClosestPart(Player,List)
        local shortestDistance = math.huge
        local closestPart
        if Player.Character and Player.Character:FindFirstChild("Humanoid") and Player.Character:FindFirstChild("Head") and Player.Character.Humanoid.Health ~= 0 and Player.Character:FindFirstChild("HumanoidRootPart") then
            for i, v in pairs(Player.Character:GetChildren()) do
                if v:IsA("BasePart") then 
                    local pos = Camera:WorldToViewportPoint(v.Position)
                    local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(Mouse.X, Mouse.Y)).magnitude
                        if magnitude < shortestDistance and table.find(List,v.Name) then
                            closestPart = v
                            shortestDistance = magnitude
                        end
                    end
                end 
            return closestPart
        end
    end 
    
    function FOVCHECK(Player,FOV) 
        if FOV.Visible == false then return true end 

        local FOVCheck = FOV.Radius

        
        if Player ~= LocalPlayer and Player.Character and Player.Character:FindFirstChild("Humanoid") and Player.Character.Humanoid.Health ~= 0 and Player.Character:FindFirstChild("LowerTorso") then
        local pos = Camera:WorldToViewportPoint(Player.Character.PrimaryPart.Position)
        local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(Mouse.X, Mouse.Y)).magnitude
            if magnitude < FOVCheck then
                FOVCheck = magnitude 
                return true 
            else 
                return false 
            end
        end
    end 



    Angle = 0 

    local function SilentAwwwim(Radius)
        local shortestDistance = Radius

        local closestPlayer
        for i, v in pairs(game.Players:GetPlayers()) do
            if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Head") and v.Character.Humanoid.Health ~= 0 and v.Character:FindFirstChild("HumanoidRootPart") then
                local pos = Camera:WorldToViewportPoint(v.Character.HumanoidRootPart.Position)
                    local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(Mouse.X, Mouse.Y)).magnitude
                        if magnitude < shortestDistance then
                            closestPlayer = v
                            shortestDistance = magnitude
                        end
                    end
                end 
        return closestPlayer
    end
    

    client_info.fps_changed(function(fps)
        
        PingLol = math.floor(game:GetService('Stats').Network.ServerStatsItem["Data Ping"]:GetValue())*0.01

        if game:GetService('Stats').Network.ServerStatsItem["Data Ping"]:GetValue() >= 105 then 
            PingLol = 105*0.01 
        end     
        
        Watermark.Title.Text = (WatermarkSettings.Name .." | fps: " .. fps .." | ping: " .. math.floor(game:GetService('Stats').Network.ServerStatsItem["Data Ping"]:GetValue()) .. "        | " .. os.date("%H") .. ":" ..os.date("%M"))
        Watermark.Outline.Size = Vector2.new(Watermark.Title.TextBounds.X + 14, 25)
        Watermark.Inline.Size = Vector2.new(Watermark.Outline.Size.X - 2, Watermark.Outline.Size.Y - 2)
        Watermark.Frame.Size = Vector2.new(Watermark.Inline.Size.X - 2, Watermark.Inline.Size.Y - 2)
        Watermark.Accent.Size = Vector2.new(Watermark.Frame.Size.X, 2)
        Ping.Bar.Size = Vector2.new(Ping.Outline.Size.X * (PingLol),  8)
        --Positioning
        Watermark.Outline.Position = Vector2.new(25, 41)
        Watermark.Inline.Position = Vector2.new(Watermark.Outline.Position.X + 1, Watermark.Outline.Position.Y + 1)
        Watermark.Frame.Position = Vector2.new(Watermark.Inline.Position.X + 1, Watermark.Inline.Position.Y + 1)
        Watermark.Accent.Position = Vector2.new(Watermark.Frame.Position.X, Watermark.Frame.Position.Y)
        Watermark.Title.Position = Vector2.new(Watermark.Frame.Position.X + 5, Watermark.Frame.Position.Y + 5)
        Ping.Bar.Position = Vector2.new(Watermark.Title.Position.X + Watermark.Title.TextBounds.X - 95, Watermark.Frame.Position.Y + 8)
        Ping.Outline.Position = Vector2.new(Watermark.Title.Position.X + Watermark.Title.TextBounds.X - 95, Watermark.Frame.Position.Y + 8)
    end)


    SpinAngle = 0 
    game:GetService("RunService").heartbeat:Connect(LPH_NO_VIRTUALIZE(function(FPS)
        if library.flags["spinning_cursor"] then 
            SpinAngle += rad(library.flags["spin_speed"]*FPS)
        end 

        if library.flags["spin"] then 
            LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(library.flags["spin_speed"]),0)
            LocalPlayer.Character.Humanoid.AutoRotate = false
        end 

        if library.flags["fly"] and library.flags["fly_key"] then 
            local FlyPosition = Vector3.new(0,0,0)
            local CCV = game.Workspace.CurrentCamera.CFrame.lookVector
        
            if UIS:IsKeyDown(Enum.KeyCode.W) then
                FlyPosition = FlyPosition + CCV
            end
        
            if UIS:IsKeyDown(Enum.KeyCode.S) then
                FlyPosition = FlyPosition - CCV
            end
        
            if UIS:IsKeyDown(Enum.KeyCode.D) then
                FlyPosition = FlyPosition + Vector3.new(-CCV.Z, 0, CCV.X)
            end
        
            if UIS:IsKeyDown(Enum.KeyCode.A) then
                FlyPosition = FlyPosition + Vector3.new(CCV.Z, 0, -CCV.x)
            end
            
            if UIS:IsKeyDown(Enum.KeyCode.Space) then
                FlyPosition = FlyPosition + Vector3.new(0, 1, 0)
            end
            
            if UIS:IsKeyDown(Enum.KeyCode.LeftShift) then
                FlyPosition = FlyPosition - Vector3.new(0, 1, 0)
            end
            
            if FlyPosition.Unit.y == FlyPosition.Unit.y then
                LocalPlayer.Character.HumanoidRootPart.Anchored = false 
                LocalPlayer.Character.HumanoidRootPart.Velocity = FlyPosition.Unit * library.flags["fly_speed"]
            else 
                LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                LocalPlayer.Character.HumanoidRootPart.Anchored = true
            end 
        end 

        if library.flags["jitter"] then 
            local RandomJit =  math.random(30, 90)
            LocalPlayer.Character.Humanoid.AutoRotate = false
            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(LocalPlayer.Character.HumanoidRootPart.CFrame.Position) * CFrame.Angles(0, math.rad(library.flags["jitter_angle"]) + math.rad((math.random(1, 2) == 1 and RandomJit or -RandomJit)), 0) 
        end 

        
        if library.flags["velocity_desync"] then 
            getgenv().OldVelocity = LocalPlayer.Character.HumanoidRootPart.Velocity

            LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(0.001),0)

            if library.flags["velocity_type"] == "Manual" then 
                LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(library.flags["Velocity_X"]/2500,library.flags["Velocity_Y"]/2500,library.flags["Velocity_Z"]/2500) * 2^16
            elseif library.flags["velocity_type"] == "Disable Prediction" then 
                LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
            elseif library.flags["velocity_type"] == "Unhittable" then
                LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(1,1,1) * -(2^library.flags["unhittable_power"])
                LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(0.001),0)
            end
            getgenv().Visualizer = LocalPlayer.Character.HumanoidRootPart.Velocity

            if library.flags["visualize_velocity"] then 
                if getgenv().Visualizer ~= nil then 
                    Visualizevelocity.HumanoidRootPart.CFrame = CFrame.new(LocalPlayer.Character.HumanoidRootPart.Position + getgenv().Visualizer * 0.1413)
                end 
            end 

            game:GetService("RunService").RenderStepped:Wait()

            LocalPlayer.Character.HumanoidRootPart.Velocity = getgenv().OldVelocity
        end 
        
        if library.flags["cframe_desync"] then 
            getgenv().OldPosition = LocalPlayer.Character.HumanoidRootPart.CFrame
            if library.flags["cframe_desync_key"]  then 
                if library.flags["custom_cframe"] then 
                    LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(library.flags["CFrame_X"],library.flags["CFrame_Y"],library.flags["CFrame_Z"]) * CFrame.Angles(rad(library.flags["pitch"]),rad(library.flags["yaw"]),rad(library.flags["roll"]))
                    if library.flags["visualize_cframe"] then 
                        Visualizecframe.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(library.flags["CFrame_X"],library.flags["CFrame_Y"],library.flags["CFrame_Z"]) * CFrame.Angles(rad(library.flags["pitch"]),rad(library.flags["yaw"]),rad(library.flags["roll"]))
                    end 
                end 
        
                if library.flags["CFrame_Type"] == "Random" then 
                    local TargetPos = LocalPlayer.Character.HumanoidRootPart.Position
                    local Random_CFrame = (CFrame.new(TargetPos) + Vector3.new(math.random(-library.flags["random_range"], library.flags["random_range"]), math.random(-library.flags["random_range"], library.flags["random_range"]), math.random(-library.flags["random_range"], library.flags["random_range"]))) * CFrame.Angles(math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)))
                    LocalPlayer.Character.HumanoidRootPart.CFrame = Random_CFrame 
                    if library.flags["visualize_cframe"] then 
                        Visualizecframe.HumanoidRootPart.CFrame = Random_CFrame
                    end 
                end 
        
                if library.flags["CFrame_Type"] == "Upside Down" then 
                    LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,0,math.rad(-180))
                    if library.flags["visualize_cframe"] then 
                        Visualizecframe.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
                    end 
                end 
        
                if library.flags["CFrame_Type"] == "Floor Plant" then 
                    local Floor_Plant = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-4,0) * CFrame.Angles(0,math.rad(math.random(1,360)),math.rad(-180))
                    LocalPlayer.Character.HumanoidRootPart.CFrame = Floor_Plant
                    if library.flags["visualize_cframe"] then 
                        Visualizecframe.HumanoidRootPart.CFrame = Floor_Plant
                    end 
                end 
        
                if library.flags["CFrame_Type"] == "Sine" then 
                    local Sine_Value = math.abs(math.sin(tick() * (library.flags["sine_speed"]/5)))
                    LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(Sine_Value*360),math.rad(Sine_Value*360),math.rad(Sine_Value*360))
                    if library.flags["visualize_cframe"] then 
                        Visualizecframe.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(Sine_Value*360),math.rad(Sine_Value*360),math.rad(Sine_Value*360))
                    end 
                end 
                game:GetService("RunService").RenderStepped:Wait()

                LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPosition
                end 
            end 

        
        
 
        if library.flags["anti_slow_down"] and LocalPlayer.Character then 
            if LocalPlayer.Character.BodyEffects.Reload.Value then
                LocalPlayer.Character.BodyEffects.Reload.Value = false
            end
            local Slowdowns = LocalPlayer.Character.BodyEffects.Movement:FindFirstChild('NoJumping') or LocalPlayer.Character.BodyEffects.Movement:FindFirstChild('NoWalkSpeed') or LocalPlayer.Character.BodyEffects.Movement:FindFirstChild('ReduceWalk')
            if Slowdowns then
                Slowdowns:Destroy()
            end
        end 

        if library.flags["anti_jump_cooldown"] and LocalPlayer.Character then 
            LocalPlayer.Character.Humanoid.UseJumpPower = false
        end 

        if library.flags["anti_bag"] then 
            if LocalPlayer.Character:FindFirstChild("Christmas_Sock") ~= nil then 
                LocalPlayer.Character["Christmas_Sock"]:Destroy()
            end 
        end 

        if library.flags["anti_stomp"] then 
            if LocalPlayer.Character.Humanoid.Health < 5 then
                for i,v in pairs(LocalPlayer.Character:GetDescendants()) do 
                    if v:IsA("BasePart") then 
                        v:Destroy()
                    end 
                end 
            end
        end 

        if library.flags["auto_pick_up_money"] then 
            for i,v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do 
                if v.Name == "MoneyDrop" then 
                    if (v.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 25 then 
                        fireclickdetector(v.ClickDetector)
                    end 
                end 
            end 
        end 

        if library.flags["auto_stomp"] then 
            game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
        end 

        if library.flags["auto_reload"] then 
            if LocalPlayer.Character:FindFirstChildWhichIsA("Tool") ~= nil then
                if LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo") then
                    if LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo").Value <= 0 then
                        game:GetService("ReplicatedStorage").MainEvent:FireServer(
                        "Reload",
                        LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
                        )
                    end
                end
            end
        end 

        if library.flags["aim_assist_enabled"] and library.flags["aim_assist_key"] then 
            if getgenv().AimAssist.Visible then 
                getgenv().InFOVRangeAimAssist = FOVCHECK(CTarget,getgenv().AimAssist) 
            else 
                getgenv().InFOVRangeAimAssist = true 
            end 
            if library.flags["aim_assist_resolver"] and library.flags["aim_assist_resolver_keybind"] then 
                getgenv().Main = Camera:WorldToScreenPoint(getgenv().CTarget.Character[library.flags["aim_assist_hitbox"]].Position + (getgenv().CTarget.Character.Humanoid.MoveDirection * library.flags["aim_assist_prediction"]))
            else 
                getgenv().Main = Camera:WorldToScreenPoint(getgenv().CTarget.Character[library.flags["aim_assist_hitbox"]].Position + (getgenv().CTarget.Character.HumanoidRootPart.Velocity * library.flags["aim_assist_prediction"]))
            end 
            if getgenv().InFOVRangeAimAssist then 
                local Offset = Vector2.new(Main.X - Mouse.X,Main.Y - Mouse.Y)
                mousemoverel(Offset.X / (library.flags["x_smoothing"] + 1), Offset.Y / (library.flags["y_smoothing"] + 1))
            end 
        end 

        for _, v in pairs(LocalPlayer.Character:GetChildren()) do
            if v:IsA("Script") and v.Name ~= "Health" and v.Name ~= "Sound" and v:FindFirstChild("LocalScript") then
                v:Destroy()
            end
        end

        if library.flags["custom_cursor"] then 
            game:GetService("UserInputService").OverrideMouseIconBehavior = Enum.OverrideMouseIconBehavior.ForceHide
            if library.flags["spinning_cursor"] then 
                getgenv().SpinSize = library.flags["cursor_gap"]

                if library.flags["sine_animation"] then 
                    getgenv().SpinSize = ((library.flags["cursor_gap"] * (abs(sin(tick() * library.flags["sine_animation_speed"]) + library.flags["sine_animation_offset"])))) 
                    print(SpinSize)
                end     
                dot.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
                outline.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
                Cursor.Line1.From = Vector2.new(Mouse.X,Mouse.Y + 36) + (Vector2.new(cos(SpinAngle), sin(SpinAngle))*(SpinSize + 1));
                Cursor.Line1.To = Cursor.Line1.From + (Vector2.new(cos(SpinAngle), sin(SpinAngle))*library.flags["cursor_size"]);
                Cursor.Line2.From = Vector2.new(Mouse.X,Mouse.Y + 36)  + (Vector2.new(cos(SpinAngle + pi/2), sin(SpinAngle + pi/2))*(SpinSize + 1));
                Cursor.Line2.To = Cursor.Line2.From + (Vector2.new(cos(SpinAngle + pi/2), sin(SpinAngle + pi/2))*library.flags["cursor_size"]);
                Cursor.Line3.From = Vector2.new(Mouse.X,Mouse.Y + 36)  + (Vector2.new(cos(SpinAngle + pi), sin(SpinAngle + pi))*(SpinSize + 1));
                Cursor.Line3.To = Cursor.Line3.From + (Vector2.new(cos(SpinAngle + pi), sin(SpinAngle + pi))*library.flags["cursor_size"]);
                Cursor.Line4.From = Vector2.new(Mouse.X,Mouse.Y + 36)  + (Vector2.new(cos(SpinAngle + pi/2*3), sin(SpinAngle + pi/2*3))*(SpinSize + 1));
                Cursor.Line4.To = Cursor.Line4.From + (Vector2.new(cos(SpinAngle + pi/2*3), sin(SpinAngle + pi/2*3))*library.flags["cursor_size"]);

                Outlines.OutlineLine1.From = Vector2.new(Mouse.X,Mouse.Y + 36) + (Vector2.new(cos(SpinAngle), sin(SpinAngle))*(SpinSize + 1));
                Outlines.OutlineLine1.To = Cursor.Line1.From + (Vector2.new(cos(SpinAngle), sin(SpinAngle))*library.flags["cursor_size"]);
                Outlines.OutlineLine2.From = Vector2.new(Mouse.X,Mouse.Y + 36)  + (Vector2.new(cos(SpinAngle + pi/2), sin(SpinAngle + pi/2))*(SpinSize + 1));
                Outlines.OutlineLine2.To = Cursor.Line2.From + (Vector2.new(cos(SpinAngle + pi/2), sin(SpinAngle + pi/2))*library.flags["cursor_size"]);
                Outlines.OutlineLine3.From = Vector2.new(Mouse.X,Mouse.Y + 36)  + (Vector2.new(cos(SpinAngle + pi), sin(SpinAngle + pi))*(SpinSize + 1));
                Outlines.OutlineLine3.To = Cursor.Line3.From + (Vector2.new(cos(SpinAngle + pi), sin(SpinAngle + pi))*library.flags["cursor_size"]);
                Outlines.OutlineLine4.From = Vector2.new(Mouse.X,Mouse.Y + 36)  + (Vector2.new(cos(SpinAngle + pi/2*3), sin(SpinAngle + pi/2*3))*(SpinSize + 1));
                Outlines.OutlineLine4.To = Cursor.Line4.From + (Vector2.new(cos(SpinAngle + pi/2*3), sin(SpinAngle + pi/2*3))*library.flags["cursor_size"]);
            end 
            
            if not library.flags["spinning_cursor"] then 
                dot.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
                outline.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
                Cursor.Line1.From = Vector2.new(Mouse.X + library.flags["cursor_gap"] * 5 , Mouse.Y + 36)
                Cursor.Line1.To = Vector2.new(Mouse.X + library.flags["cursor_size"] * 5, Mouse.Y+ 36)
                Cursor.Line2.From = Vector2.new(Mouse.X - library.flags["cursor_gap"] * 5, Mouse.Y+ 36)
                Cursor.Line2.To = Vector2.new(Mouse.X - library.flags["cursor_size"] * 5 , Mouse.Y+ 36)
                Cursor.Line3.From = Vector2.new(Mouse.X, Mouse.Y - library.flags["cursor_gap"] * 5 + 36)
                Cursor.Line3.To = Vector2.new(Mouse.X, Mouse.Y - library.flags["cursor_size"] * 5+ 36)
                Cursor.Line4.From = Vector2.new(Mouse.X, Mouse.Y + library.flags["cursor_gap"] * 5+ 36)
                Cursor.Line4.To = Vector2.new(Mouse.X, Mouse.Y + library.flags["cursor_size"] * 5+ 36)
                
                Outlines.OutlineLine1.From = Vector2.new(Mouse.X + library.flags["cursor_gap"] * 5, Mouse.Y + 36)
                Outlines.OutlineLine1.To = Vector2.new(Mouse.X + library.flags["cursor_size"] * 5, Mouse.Y+ 36)
                Outlines.OutlineLine2.From = Vector2.new(Mouse.X - library.flags["cursor_gap"] * 5, Mouse.Y+ 36)
                Outlines.OutlineLine2.To = Vector2.new(Mouse.X - library.flags["cursor_size"] * 5 , Mouse.Y+ 36)
                Outlines.OutlineLine3.From = Vector2.new(Mouse.X, Mouse.Y - library.flags["cursor_gap"] * 5 + 36)
                Outlines.OutlineLine3.To = Vector2.new(Mouse.X, Mouse.Y - library.flags["cursor_size"] * 5+ 36)
                Outlines.OutlineLine4.From = Vector2.new(Mouse.X, Mouse.Y + library.flags["cursor_gap"] * 5+ 36)
                Outlines.OutlineLine4.To = Vector2.new(Mouse.X, Mouse.Y + library.flags["cursor_size"] * 5+ 36)
            end 
        end 



        Lock.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
        AimAssist.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
        LockOutline.Position = Vector2.new(Mouse.X,Mouse.Y + 36)
        AimAssistOutline.Position = Vector2.new(Mouse.X,Mouse.Y + 36)


        if Target ~= nil and library.flags["lock_enabled"] and library.flags["lock_key"] and Target.Character and Target.Character.Humanoid and Target.Character.Humanoid.Health > 0 and not library.flags["silent_aim_enabled"] and KnockCheck(Target) then 
            getgenv().HitPart = ClosestPart(Target,library.flags["hitpart"]).Name
            getgenv().BehindWall = WallCheck(Target.Character.Head)
            getgenv().Knocked = KnockCheck(Target) 
            getgenv().InFOVRange = FOVCHECK(Target,getgenv().Lock) 


            if Target.Character.Humanoid:GetState() == Enum.HumanoidStateType.Freefall then 
                getgenv().jump_offset = library.flags["jump_offset"]
            else 
                getgenv().jump_offset = 0
            end 
        
            if library.flags["target_strafe"] then 
                Angle += library.flags["strafe_speed"]
                LocalPlayer.Character.HumanoidRootPart.CFrame = Target.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(Angle), 0) * CFrame.new(0, library.flags["strafe_height"], library.flags["strafe_radius"])
            end 

            if library.flags["look_at"] then 
                LocalPlayer.Character.Humanoid.AutoRotate = false 
                local OldCframe = LocalPlayer.Character.PrimaryPart
                local NearestRoot = Target.Character.HumanoidRootPart
                local NearestPos = CFrame.new(LocalPlayer.Character.PrimaryPart.Position, Vector3.new(NearestRoot.Position.X, OldCframe.Position.Y, NearestRoot.Position.Z))
                LocalPlayer.Character:SetPrimaryPartCFrame(NearestPos)
            end 
            
            if library.flags["part_enabled"] then 
                if library.flags["part_velocity_enabled"] then 
                    print(HitPart)
                    print(TargetPart.Name)
                    TargetPart.CFrame = Target.Character[HitPart].CFrame + (Target.Character.HumanoidRootPart.Velocity * Prediction)
                else 
                    print(HitPart)
                    print(TargetPart.Name)
                    TargetPart.CFrame = Target.Character[HitPart].CFrame
                end 
            end 

            if library.flags["part_chams"] then 
                for Index,Value in pairs(game.Workspace.Terrain:GetChildren()) do 
                    if Value:IsA("BasePart") then 
                        Value.Position = Target.Character[Value.Name].Position + Target.Character.HumanoidRootPart.Velocity * 0.1413
                        Value.Rotation = Target.Character[Value.Name].Rotation
                    end     
                end 
            end 
        end 

        if library.flags["force_time"] then 
            Lighting.TimeOfDay = ""..library.flags["time"]..":00:00"
        end 

        if LocalPlayer.Character then 
            if library.flags["cframe_speed"] and library.flags["cframe_speed_keybind"] then 
                LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame + LocalPlayer.Character.Humanoid.MoveDirection * (library.flags["cframe_speed_power"]/25)
                if library.flags["auto_jump"] and LocalPlayer.Character.Humanoid.FloorMaterial ~= Enum.Material.Air then 
                    LocalPlayer.Character.Humanoid:ChangeState("Jumping")
                    LocalPlayer.Character.Humanoid.UseJumpPower = false
                end     
            end 

            if library.flags["bhop"] then 
                if LocalPlayer.Character.Humanoid.FloorMaterial == Enum.Material.Air then 
                    LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame + LocalPlayer.Character.Humanoid.MoveDirection * (library.flags["bhop_power"]/75)
                end 
            end 

            

            if library.flags["bullet_tracers"] and tool() then 
                for i,v in pairs(game.Workspace.Ignored:GetChildren()) do 
                    if v:IsA("Part") and v.Name == "BULLET_RAYS" then 
                        if (v.Position - tool().Handle.Position).Magnitude < 10 then 
                            v:Destroy()
                        end 
                    end 
                end 
            end 
        end 

        local AccentEffect = library.flags["settings/menu/effects"];
        local EffectSpeed = library.flags["settings/menu/effect_speed"];
        if AccentEffect == "rainbow" then
            local Clock = os.clock() * EffectSpeed
            local Color = Color3.fromHSV(math.abs(math.sin(Clock)), 1, 1);
            library:ChangeThemeOption("Accent", Color);
        end;
        if AccentEffect == "shift" then
            local ShiftOffset = 0
            local Clock = os.clock() * EffectSpeed + ShiftOffset;
            ShiftOffset = ShiftOffset + 0.01;
            local Color = Color3.fromHSV(math.abs(math.sin(Clock)), 1, 1)
            library.flags["theme_accent"] = Color;
            library:ChangeThemeOption("Accent", Color);
        end;
        if AccentEffect == "reverse shift" then
            local ShiftOffset = 0
            local Clock = os.clock() * EffectSpeed + ShiftOffset;
            ShiftOffset = ShiftOffset - 0.01;
            local Color = Color3.fromHSV(math.abs(math.sin(Clock)), 1, 1)
            library.flags["theme_accent"] = Color;
            library:ChangeThemeOption("Accent", Color);
        end;
    end));



    local BeamPart = Instance.new("Part", workspace)
    BeamPart.Name = "BeamPart"
    BeamPart.Transparency = 1

    function createBeam(v1, v2)
        local colorSequence = ColorSequence.new({
        ColorSequenceKeypoint.new(0, library.flags["tracers_color"]),
        ColorSequenceKeypoint.new(1, library.flags["tracers_color"]),
        })
        local Part = Instance.new("Part", BeamPart)
        Part.Size = Vector3.new(1, 1, 1)
        Part.Transparency = 1
        Part.CanCollide = false
        Part.CFrame = CFrame.new(v1)
        Part.Anchored = true
        local Attachment = Instance.new("Attachment", Part)
        local Part2 = Instance.new("Part", BeamPart)
        Part2.Size = Vector3.new(1, 1, 1)
        Part2.Transparency = 1
        Part2.CanCollide = false
        Part2.CFrame = CFrame.new(v2)
        Part2.Anchored = true
        Part2.Color = Color3.fromRGB(255, 255, 255)
        local Attachment2 = Instance.new("Attachment", Part2)
        local Beam = Instance.new("Beam", Part)
        Beam.FaceCamera = true
        Beam.Color = colorSequence
        Beam.Attachment0 = Attachment
        Beam.Attachment1 = Attachment2
        Beam.LightEmission = 6
        Beam.LightInfluence = 1
        Beam.Width0 = 1
        Beam.Width1 = 0.6
        Beam.Texture = "rbxassetid://" .. texture
        Beam.LightEmission = 1
        Beam.LightInfluence = 1
        Beam.TextureMode = Enum.TextureMode.Wrap 
        Beam.TextureLength = 3 
        Beam.TextureSpeed = 3
        delay(1, function()
        for i = 0.5, 1, 0.02 do
        wait()
        Beam.Transparency = NumberSequence.new(i)
        end
        Part:Destroy()
        Part2:Destroy()
        end)
    end

    local function Chance(percentage)
        local Chance = percentage 

        if math.random(1,100) <= percentage then 
            return true 
        else 
            return false
        end 
    end 

    function WallCheck(head)
        if not table.find(library.flags["checks"], "Wall Check") then return false end 
        if v == LocalPlayer then return false end
            local castPoints = {LocalPlayer.Character.Head.Position, head.Position}
            local ignoreList = {LocalPlayer.Character,head.Parent}
            a = game.Workspace.CurrentCamera:GetPartsObscuringTarget(castPoints, ignoreList)
            if #a == 0 then return false end
            return true
        end 

        function KnockCheck(head)
            if not table.find(library.flags["checks"], "Knock Check") then return true end 

            if head:FindFirstChild("Humanoid") and head.BodyEffects["K.O"].Value == true then 
                return false 
            else
                return true
            end 
        end
    --[[
        Old Silent Aim method removed due to alot of shots missing
    local Old;
    Old = hookmetamethod(game, "__index", LPH_NO_VIRTUALIZE(function(self, k)
        if Target ~= nil and Hit ~= nil and Chance(library.flags["hit_chance"]) and Knocked and not BehindWall and InFOVRange then 
            if (self:IsA("Mouse") and (k == "Hit" or k == "Target")) then 
                return (k == "Hit" and getgenv().Hit)
            end
        end 
        return Old(self, k)
    end))
      ]]


