local drawingPool =         {}
local function newDrawing(type, prop)
local obj = Drawing.new(type)
    if prop then
        for i,v in next, prop do
            obj[i] = v
        end
    end
    return obj  
end

for i,v in next, drawingPool do
    if not v.used then
        v.used = true
            drawing = v
        break
    end
end

