            VisualizerColorVelocity(Color3.new(1,1,1),0.5)

            function ChamColor(Player,Color1,Color2)
                if ESPEnabled.Chams[1] then 
                    for i,v in pairs(Player.Character:GetDescendants()) do 
                        if v:IsA("BoxHandleAdornment") and v.Name == "Box1" then 
                            v.Color3 = Color1 
                        end 

                        if v:IsA("BoxHandleAdornment") and v.Name == "Box2" then 
                            v.Color3 = Color2
                        end 
                    end 
                end 
            end 

            local function AddCham(Player)
                if Player ~= game.Players.LocalPlayer and ESPEnabled.Chams[1] and Player.Character and Player.Character.Head then 
                    for n,m in next, bodyParts do 
                        if m ~= "HumanoidRootPart" then 
                            Box1 = Instance.new("BoxHandleAdornment", Player.Character[m])
                            Box1.Name = "Box1"
                            Box1.AlwaysOnTop = false
                            Box1.ZIndex = 1
                            Box1.Color3 = ESPEnabled.Chams[2]
                            Box1.Transparency = ESPEnabled.Chams[4]
                            Box1.Size = Player.Character[m].Size + Vector3.new(0.02,0.02,0.02)
                            Box1.Adornee = Player.Character[m]
            
                            Box2 = Instance.new("BoxHandleAdornment", Player.Character[m])
                            Box2.Name = "Box2"
                            Box2.AlwaysOnTop = true
                            Box2.ZIndex = 1
                            Box2.Color3 = ESPEnabled.Chams[3]
                            Box2.Transparency = ESPEnabled.Chams[5]
                            Box2.Size = Player.Character[m].Size + Vector3.new(0.01,0.01,0.01)
                            Box2.Adornee = Player.Character[m]
                        end 
                    end
                end 
            end 
        


