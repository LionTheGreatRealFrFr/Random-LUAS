                    lock_section:new_slider({flag = "jump_offset", name = "Jump Offset", min = -1, max = 1, default = 0, float = 0.01})
                    lock_section:new_toggle({name = "Resolver", flag = 'resolver'})
                    lock_section:new_keybind({name = "Resolver Keybind", flag = 'resolver_keybind',keybind_name = "Lock Resolver", mode = "Toggle", ignore = false})
