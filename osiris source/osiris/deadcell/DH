            Args[3] = Target.Character["HumanoidRootPart"].CFrame + (Target.Character.HumanoidRootPart.Velocity * library.flags["lock_prediction"])  

            return Old(unpack(Args))
        end 
        return Old(...)
    end))]]


    --[[local RawMT = getrawmetatable(game)
    local Old = RawMT.__namecall
    setreadonly(RawMT, false)
    RawMT.__namecall = LPH_NO_VIRTUALIZE(function(...)
        local args = {...}
            if getnamecallmethod() == "FireServer"  and args[2] == "UpdateMousePos" and Target ~= nil and library.flags["lock_enabled"] and Target.Character then
                    if not library.flags["lock_resolver"] then 
                        args[3] = Target.Character[HitPart].Position + (Target.Character.HumanoidRootPart.Velocity *  library.flags["lock_prediction"]) + Vector3.new(0.1,jump_offset,0.1)
                    else 
                        args[3] = Target.Character[HitPart].Position + (Target.Character.Humanoid.MoveDirection *  library.flags["lock_prediction"]*19.64285714289) + Vector3.new(0.1,jump_offset,0.1)
                    end 
                return Old(unpack(args))
