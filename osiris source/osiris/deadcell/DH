                                Sliders["Health"]:Set(Z / 100, (math.floor(Z / 0.01) * 0.01) .. "")
                            end 

                            if library.flags["lock_enabled"] and library.flags["lock_type"] == "Silent Aim" then 
                                getgenv().Target = getClosestPlayerToCursor(math.huge)
                            end 
                        end 
                    end)
                    
