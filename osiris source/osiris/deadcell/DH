                for i,v in next, Visualizevelocity:GetChildren() do 
                    if v.Name ~= "HumanoidRootPart" then 
                        if v:IsA("BasePart") then 
                            v.Color = Color
                            v.Transparency = Transparency
                        end 
                    end 
                end
            end 

