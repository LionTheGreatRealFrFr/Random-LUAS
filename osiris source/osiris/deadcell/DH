                        while true do 
                            task.wait(0.1)

                            if Target ~= nil and library.flags["lock_enabled"] and library.flags["lock_key"] and Target.Character and Target.Character.Humanoid and Target.Character.Humanoid.Health > 0 and not library.flags["silent_aim_enabled"] and KnockCheck(Target) and library.flags["target_stats"] then 
