            function RemoveCham(Player)
                for i,v in pairs(Player.Character:GetDescendants()) do 
                    if v:IsA("BoxHandleAdornment") and v.Name == "Box1" or v.Name == "Box2" then 
                        v:Destroy()
                    end 
                end 
