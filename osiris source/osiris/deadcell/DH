        local function getClosestPlayerToCursor(Radius)
            local shortestDistance = Radius

            local closestPlayer
            for i, v in pairs(game.Players:GetPlayers()) do
                if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Head") and v.Character.Humanoid.Health ~= 0 and v.Character:FindFirstChild("HumanoidRootPart") then
                    local pos,OnScreen = Camera:WorldToViewportPoint(v.Character.HumanoidRootPart.Position)
                        local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(Mouse.X, Mouse.Y)).magnitude
                            if magnitude < shortestDistance and OnScreen then
                                closestPlayer = v
                                shortestDistance = magnitude
                            end
                        end
                    end 
            return closestPlayer
        end


        -- combat
        do
            local combat_main = combat:new_section_holder({name = "combat", size = "Fill"})  
                do 
                    local aim_assist = combat_main:new_section({name = 'Aim Assist'}) 
                    aim_assist:new_toggle({name = "Enabled", flag = 'aim_assist_enabled'})
                    aim_assist:new_keybind({name = "Key", keybind_name = "Aim Assist", flag = 'aim_assist_key',ignore = false, mode = "Toggle",callback = function(a)
                        if library.flags["aim_assist_enabled"] and library.flags["aim_assist_key"] then 
                            getgenv().CTarget = getClosestPlayerToCursor(math.huge)
                        else 
                            getgenv().CTarget = nil 
                        end 
                    end})
                    aim_assist:new_textbox({placeholder = "Prediction", middle = true, flag = 'aim_assist_prediction'})
                    aim_assist:new_slider({name = "Horizontal Smoothing", flag = 'x_smoothing', min = 0, max = 100, default = 5, float = 0.1})
                    aim_assist:new_slider({name = "Vertical Smoothing", flag = 'y_smoothing', min = 0, max = 100, default = 5, float = 0.1})
                    aim_assist:new_dropdown({name = "Hitpart", flag = 'aim_assist_hitbox', options = bodyParts, default = 'HumanoidRootPart'})
                    aim_assist:new_toggle({name = "Resolver", flag = 'aim_assist_resolver'})
                    aim_assist:new_keybind({name = "Keybind", flag = 'aim_assist_resolver_keybind',keybind_name = "Resolver", mode = "Toggle", ignore = false})
                    aim_assist:new_seperator({name = "FOV"})
                    --
                    aim_assist:new_toggle({name = "Render Field Of View", flag = 'aim_assist_render_field_of_view',callback = function(a)
                        AimAssist.Visible = a
                        AimAssist.Position = Vector2.new(Camera.ViewportSize.X/2,Camera.ViewportSize.Y/2)
                    end}):new_colorpicker({flag = 'aim_assist_fov_color', default = Color3.fromRGB(255,255,255),callback = function(a)
                        AimAssist.Color = a
                    end})
                    --
                    aim_assist:new_toggle({name = "Render Outline", flag = 'aim_assist_render_outline',callback = function(a)
                        AimAssistOutline.Visible = a
                    end}):new_colorpicker({flag = 'aim_assist_outline_color', default = Color3.fromRGB(0,0,0),callback = function(a)
                        AimAssistOutline.Color = a
                    end})
                    --
                    aim_assist:new_toggle({name = "Filled", flag = 'aim_assist_render_outline',callback = function(a)
                        AimAssist.Filled = a
                    end})
                    --
                    aim_assist:new_slider({name = "Radius (°)", flag = 'aim_assist_radius', min = 0, max = 1000, default = 100, float = 1,callback = function(a)
                        AimAssist.Radius = a
                        AimAssistOutline.Radius = a
                    end})
                    --
                    aim_assist:new_slider({name = "Sides", flag = 'aim_assist_fov_sides', min = 0, max = 20, default = 0, float = 1,callback = function(a)
                        AimAssist.NumSides = a
                        AimAssistOutline.NumSides = a
                    end})
                    --
                    aim_assist:new_slider({name = "Transparency (%)", flag = 'aim_assist_fov_transparency', min = 0, max = 1, default = 0, float = 0.01,callback = function(a)
                        AimAssist.Transparency = (1 - a)
                        AimAssistOutline.Transparency = (1 - a)
                    end})
                    --
                end

                do
                    local lock_section = combat_main:new_section({name = "Lock"}) 
                    lock_section:new_toggle({name = "Enabled", flag = 'lock_enabled'})
                    lock_section:new_dropdown({name = "Type", flag = 'lock_type', options = {"Lock","Silent Aim"},default = "Lock"})

                    function Clone(Player,Color,Material,Transparency)
                        for i,v in pairs(Player.Character:GetChildren()) do 
                            if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then 
                                local ClonedPart = Instance.new("Part")
                                ClonedPart.Anchored = true 
                                ClonedPart.CanCollide = false 
                                ClonedPart.Position = v.Position
                                ClonedPart.Parent = game.Workspace.Terrain 
                                ClonedPart.Material = Enum.Material[Material]
                                ClonedPart.Shape = Enum.PartType.Block 
                                ClonedPart.Transparency = Transparency 
                                ClonedPart.Color = Color
                                ClonedPart.Size = v.Size + Vector3.new(0.01,0.01,0.01)
                                ClonedPart.Name = v.Name
                                ClonedPart.Rotation = v.Rotation
                            end 
                        end
                    end  

                    lock_section:new_keybind({name = "Key", keybind_name = "Locking", flag = 'lock_key', mode = "Toggle",default = Enum.KeyCode.E,callback = function(a)
                        if library.flags["lock_enabled"] and library.flags["lock_type"] == "Lock" then 
                            if a then 
                                getgenv().Target = getClosestPlayerToCursor(math.huge) 
                                library.notify("Locked onto:"..getgenv().Target.Name.."", 2)

                                if library.flags["view_target"] then 
                                    Camera.CameraSubject = getgenv().Target.Character.Humanoid
                                end 

                                if library.flags["part_chams"] then 
                                    Clone(getgenv().Target,library.flags["part_chams_color"],library.flags["part_chams_material"],library.flags["part_chams_transparency"])
                                end 

                                if library.flags["target_stats"] then 
                                    local pImageData = game:GetService("HttpService"):JSONDecode(game:HttpGet(("https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds=%s&size=352x352&format=Png&isCircular=false"):format(getgenv().Target.UserId)))
                                    local imagedata  = game:HttpGet((pImageData["data"][1]["imageUrl"]))
                                    Sliders["Health"]:SetVisible(true)
                                    TargetDrawing.Data = imagedata
                                    Title.Text = "".. getgenv().Target.Name .." [" .. getgenv().Target.DisplayName .."]"
                                end 
                            else 

                                library.notify("Unlocked", 2)
                                Sliders["Health"]:SetVisible(false)
                                LocalPlayer.Character.Humanoid.AutoRotate = true 
                                TargetPart.CFrame = CFrame.new(9999,9999,9999)
                                Camera.CameraSubject = LocalPlayer.Character.Humanoid
                                game.Workspace.Terrain:ClearAllChildren() 
                                getgenv().Target = nil 
                            end 
                        end 
                    end})
                    lock_section:new_textbox({placeholder = "Prediction", middle = true, flag = 'lock_prediction',default = "0.1413"})
                    lock_section:new_dropdown({name = "Hitpart", flag = 'hitpart', options = bodyParts, max = math.huge, default = {"Head","HumanoidRootPart"}})
                    lock_section:new_dropdown({name = "Checks", flag = 'checks', options = {"Wall Check", "Knock Check"}, max = math.huge})
                    lock_section:new_toggle({name = "Target Stats", flag = 'target_stats'}):new_colorpicker({flag = 'target_stats_color', default = Color3.fromRGB(102,0,255),callback = function(a)
                        Value_Slider.Color = a 
                        Accent.Color = a 
