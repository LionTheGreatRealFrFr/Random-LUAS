        Value_Slider:Remove()
        Value_Frame:Remove()
        Value_Inline:Remove()
        Value_Outline:Remove()
        Accent_Gradient:Remove() 
        Accent:Remove() 
        Frame:Remove() 
        Inline:Remove() 
        Outline:Remove() 
        getgenv().Title:Remove() 
    end 
    --
    Sliders[Value] = Slider
end

