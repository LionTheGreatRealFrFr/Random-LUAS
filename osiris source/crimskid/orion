worldeffects:AddToggle({
	Name = "World Effects",
	Default = false,
	Callback = function(v)
		_G.worldeffects = v 
		
					    if _G.worldeffects == true then 
	        ColorCorrectionEffect.Enabled = true

					    end
    
    					    if _G.worldeffects == false then 
	        ColorCorrectionEffect.Enabled = false
    game:GetService("Lighting").ColorCorrection.TintColor = Color3.fromRGB(255, 255, 255)
  game:GetService("Lighting").FogColor = Color3.new(0.392157,0.341176,0.282353)
    game:GetService("Lighting").OutdoorAmbient = Color3.new(0.596078,0.596078,0.572549)

	    end
	end    
})

worldeffects:AddColorpicker({
	Name = "Ambient",
	Default = Color3.fromRGB(0, 255, 0),
	Callback = function(Value)
	    if _G.worldeffects == true then 
	            game:GetService("Lighting").OutdoorAmbient = Value
	    end 
	end	  
})

worldeffects:AddColorpicker({
	Name = "Tint",
	Default = Color3.fromRGB(0, 255, 0),
	Callback = function(Value)
	    	    if _G.worldeffects == true then 
	    	        game:GetService("Lighting").ColorCorrection.TintColor = Value
	    end 
	end	  
})

worldeffects:AddColorpicker({
	Name = "Shadows",
	Default = Color3.fromRGB(0, 255, 0),
	Callback = function(Value)
	    	    if _G.worldeffects == true then 
	      game:GetService("Lighting").FogColor = Value

	    end 
	end	  
})

worldeffects:AddSlider({
	Name = "Saturation",
	Min = 0,
	Max = 10,
	Default = 0,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "",
	Callback = function(v)
							    if _G.worldeffects == true then 
	        
ColorCorrectionEffect.Saturation = v/5
	    end
	end    
})


local AnimationDances = Animations:AddSection({
	Name = "Dances"
