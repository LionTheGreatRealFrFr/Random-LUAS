	Default = false,
	Callback = function(v)
		    _G.rainboww  = v 
    
    while _G.rainboww == true do 
        task.wait()
     esp.arrow[2] = chromaColor
     esp.angle[2] = chromaColor
       esp.textlayout.distance.color  = chromaColor
         esp.textlayout.distance.color  = chromaColor
          esp.textlayout.name.color = chromaColor
          esp.barlayout.health.color2 = chromaColor
          esp.boxfill[2] = chromaColor
           esp.box[2]  = chromaColor
           
    end 
	end    
})

ESP:AddColorpicker({
	Name = "All Colour",
	Default = Color3.fromRGB(255, 255, 255),
	Callback = function(Value)
		    esp.arrow[2] = Value
     esp.angle[2] = Value
       esp.textlayout.distance.color  = Value
         esp.textlayout.distance.color  = Value
          esp.textlayout.name.color = Value
          esp.barlayout.health.color2 = Value
          esp.boxfill[2] = Value
           esp.box[2]  = Value
	end	  
})


local ColorCorrectionEffect = Instance.new("ColorCorrectionEffect")
ColorCorrectionEffect.Brightness = 0
ColorCorrectionEffect.Contrast = 0
ColorCorrectionEffect.Enabled = false
ColorCorrectionEffect.Saturation = 0
ColorCorrectionEffect.TintColor = Color3.fromRGB(255, 255, 255)
ColorCorrectionEffect.Parent = game:GetService("Lighting")


