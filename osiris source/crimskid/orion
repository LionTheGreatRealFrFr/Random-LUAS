	Name = "Jitter Angle",
	Min = 0,
	Max = 360,
	Default = 360,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	Callback = function(Value)
		_G.pwangle = Value
	end    
})


_G.floatheight = 20 
Movement:AddToggle({
	Name = "Float",
	Callback = function(v)
FloatToggle = v
    


while FloatToggle == true do 
    task.wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,_G.floatheight,0)
    wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-_G.floatheight,0)
end 

  	end    
})


