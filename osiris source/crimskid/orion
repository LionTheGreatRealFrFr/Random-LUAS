--[[

while toggles.CFrameSpeed == true do 
        task.wait() 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.Humanoid.MoveDirection * getfenv().Speed
            
            
        end


]]

Movement:AddToggle({
	Name = "CFrame Speed",
	Save = true,
	Callback = function(a)
	    CFrameSpeed = a 
	    
	    while CFrameSpeed == true do 
        task.wait() 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.Humanoid.MoveDirection * getfenv().Speed
            
            
        end
end})
	

Movement:AddToggle({
	Name = "Fake Macro",
	Save = true,
	Callback = function(a)
getgenv().FakeMacro = a 

while getgenv().FakeMacro == true do 
    task.wait()
    getgenv().FakeMacroTurnWait = math.random(1,5)
    if CFrameSpeed == true then 
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(90),0)
        task.wait(0.05)
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(-90),0)
          task.wait(0.05)
                   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(-90),0)
          task.wait(0.05)
                             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(90),0)
          task.wait(0.05)
    end 
end 
end}) 


Movement:AddBind({
	Name = "Bind",
	Default = "",
	Hold = false,
	Callback = function()
                            
                            



		CFrameSpeed = not CFrameSpeed
		
 		if CFrameSpeed == true then 
 		    		    		             Notify({
                Title = "Crimskid",
                Description = "Speed Turned On",
                Duration = 3
            })
        elseif CFrameSpeed == false then 
 		    		    		             Notify({
                Title = "Crimskid",
                Description = "Speed Turned Off",
                Duration = 3
            }) 
 		end
	 
	 
		while CFrameSpeed do 
    task.wait()
    
        if CFrameSpeed == true then 
            
        
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.Humanoid.MoveDirection * getfenv().Speed
 

        end 
end 



	end    
})



    local chromaColor
    spawn(function()
        while wait() do
            chromaColor = Color3.fromHSV(tick() % 6 / 6, 1, 1)
        end
    end)

Movement:AddSlider({
	Name = "Speed",
	Min = 0,
	Max = 100,
	Default = 30,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	Callback = function(Value)
		getfenv().Speed = Value/30
				getfenv().SpeedKeybind = Value/300

	end    
})

_G.JumpHeight = 60;
Movement:AddToggle({
	Name = "Infinite Jump",
	Callback = function(v)
local Player = game:GetService'Players'.LocalPlayer;
local UIS = game:GetService'UserInputService';

     _G.infinitejump = v
    function Action(Object, Function) if Object ~= nil then Function(Object); end end

    UIS.InputBegan:connect(function(UserInput)
        if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space and _G.infinitejump == true  then
            Action(Player.Character.Humanoid, function(self)
                if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                    Action(self.Parent.HumanoidRootPart, function(self)
                        self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
                    end)
                end
            end)
        end
    end)
  	end    
})

Movement:AddToggle({
	Name = "Noclip",
	Callback = function(v)
	     _G.nocliptoggle = v   
    
        while _G.nocliptoggle == true do 
            wait()
        game:GetService("Players").LocalPlayer.Character.Head.Touched:connect(function(obj)
        if obj ~= workspace.Terrain and _G.nocliptoggle == true  then 
        obj.CanCollide = false
        wait(30)
        obj.CanCollide = true
        end
        end) 
    end
    
end}) 

Movement:AddBind({
	Name = "Noclip Bind",
	Default = "",
	Hold = false,
	Callback = function()

	    
 poopnoclip = not poopnoclip   
    
    	    	if poopnoclip == true then 
 		    		    		             Notify({
                Title = "Crimskid",
