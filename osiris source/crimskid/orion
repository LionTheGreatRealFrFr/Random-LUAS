Animations:AddToggle({
	Name = "Floss",
	Default = false,
	Callback = function(v)

local Dance = Instance.new("Animation")
Dance.AnimationId = "rbxassetid://5917459365"
getgenv().DanceAnimation = v 
        if getgenv().DanceAnimation == true then
            LoadedAnim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Dance)
            LoadedAnim:Play()
            end 
        if getgenv().DanceAnimation == false then 
          pcall(function()
            LoadedAnim:Stop()
        end)
end 
 
end}) 

