AntiAims:AddToggle({
	Name = "Legit AA",
	Default = false,
	Callback = function(v)
	    getgenv().VelocityAntiAim = v 
	    	    local HipHeight = 2.2
local VelocityValue = 25

	    while getgenv().VelocityAntiAim == true do
    task.wait()
    local OldVel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(OldVel.X, -VelocityValue, OldVel.Z)
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(OldVel.X, OldVel.Y, OldVel.Z)
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(OldVel.X, -VelocityValue, OldVel.Z)
    game.Players.LocalPlayer.Character.Humanoid.HipHeight = HipHeight
   end
end})



getgenv().SpinPower = 200 
AntiAims:AddToggle({
	Name = "Velocity Spin",
	Default = false,
	Callback = function(v)
	    getgenv().SpinAntiAim = v 
while getgenv().SpinAntiAim == true do 
    task.wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,getgenv().SpinPower)
    task.wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,-getgenv().SpinPower)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(90),0)
 
end 
	            
end}) 

