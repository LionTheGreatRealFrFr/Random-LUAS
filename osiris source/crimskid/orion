local worldeffects = Visuals:AddSection({
	Name = "World Effects"
})

worldeffects:AddToggle({
	Name = "Low GFX",
	Callback = function(v)
	    if v == true then 
	        game:GetService("Lighting").GlobalShadows = false 
	    end 
   
   	    if v == false then 
	        game:GetService("Lighting").GlobalShadows = true 
	    end 
   
   
end}) 


