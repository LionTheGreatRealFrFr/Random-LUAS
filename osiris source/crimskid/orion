-- Settings

AntiAims:AddToggle({
	Name = "Camera",
	Default = false,
	Callback = function(v)

getgenv().CameraToggleeee = v
--// Variables
local plrs = game:GetService("Players")
   local ts = game:GetService("TweenService")
   local rs = game:GetService("RunService")
   local ws = game:GetService("Workspace")
   
   --// LocalPlayer
   local plr = plrs.LocalPlayer
   
   --//Making Camera
   local Camera = Instance.new("Part", ws)
       Camera.CanCollide = false
       Camera.Color = Color3.fromRGB(0, 0, 0)
       Camera.Size = Vector3.new(0.1, 0.1, 0.1)
       Camera.Anchored = true
       Camera.Transparency = 1
       Camera.Position = (plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")) and plr.Character:FindFirstChild("HumanoidRootPart").Position or Vector3.new(0, 0, 0)
       
       --//Setting the Camera
       ws.CurrentCamera.CameraSubject = Camera
       
       --//Fast Loop
       rs.RenderStepped:Connect(function()
           
           --//Fast Wait
           rs.RenderStepped:Wait()
           
           --//Other Variables
           local chr = plr.Character
                   local hrp = chr:FindFirstChild("Head")
                   local OldPos
                   
                   --//Check If On
                   if getgenv().CameraToggleeee == true then
                   --//Incase Player Dies
           ws.CurrentCamera.CameraSubject = Camera
           
                   --//Tweening The Camera
                   ts:Create(Camera, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = (OldPos or CFrame.new(hrp.CFrame.p))}):Play()
                   else
                       
                       --//Changing to hrp
                       ws.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
                       end
           end)

end}) 




   




 
getgenv().SpinAntiAim = false 
getgenv().SpinPower = 200 

 

getgenv().FallAA = false 
function FallAA()
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").StateChanged:Connect(function(alter,poo)
    if poo == Enum.HumanoidStateType.Freefall == true and getgenv().FallAA == true then
        wait(0.27)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -14, 0)
    end
end)
    
end 




getgenv().VelocityAntiAim = false
local HipHeight = 3
local VelocityValue = 80



   --
 



getgenv().aa = false
getgenv().BlatantAApowerg = 15 
function FlingAA()
    
     local Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
       
    while getgenv().aa == true and getgenv().antiaimtoggle == true do
        task.wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (CFrame.new(Position) + Vector3.new(math.random(- getgenv().BlatantAApowerg,  getgenv().BlatantAApowerg), math.random(- getgenv().BlatantAApowerg,  getgenv().BlatantAApowerg), math.random(- getgenv().BlatantAApowerg,  getgenv().BlatantAApowerg))) * CFrame.Angles(math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)))
    end

    
end 


function destroyaa()
    	        getgenv().SpinAntiAim = false  
	        getgenv().aa = false
	        getgenv().StillAA = false 
          _G.desync = false 
	        getgenv().VelocityAntiAim = false
	       
for i=1,20 do
wait(0.01)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldpos * CFrame.new(0,0,0)
end
end 

getgenv().StillAA = false 
function Stillaa()
local oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame 

while getgenv().StillAA == true do 
        task.wait()
        if getgenv().antiaimtoggle == true then 
    local x  = math.random(-9999,9999)
    local y  = math.random(-9999,9999)
    local z  = math.random(-9999,9999)
game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(x,y,z) 
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldpos * CFrame.new(0,0,0)
end 
end 
end 


getgenv().BlatantAApowerg = 15 
AntiAims:AddToggle({
	Name = "Fling",
	Default = false,
	Callback = function(v)


getgenv().aa = v
 
     local Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
       
    while getgenv().aa == true do
        task.wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (CFrame.new(Position) + Vector3.new(math.random(- getgenv().BlatantAApowerg,  getgenv().BlatantAApowerg), math.random(- getgenv().BlatantAApowerg,  getgenv().BlatantAApowerg), math.random(- getgenv().BlatantAApowerg,  getgenv().BlatantAApowerg))) * CFrame.Angles(math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)))
    end

    
end}) 


