	Name = "Custom SkyBox",
	Callback = function(v)
	    getgenv().CustomSkyBox = v 
	    
	    game:GetService("RunService").RenderStepped:Connect(function()
	        if getgenv().CustomSkyBox == true then 
        game:GetService("Lighting").TimeOfDay = "12:00:00"
end 
    end)
 

end}) 

