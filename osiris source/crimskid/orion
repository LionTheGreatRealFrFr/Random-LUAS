            })
        elseif poopnoclip == false then 
 		    		    		             Notify({
                Title = "Crimskid",
                Description = "Noclip Turned Off",
                Duration = 3
            }) 
        end
 	
 	
        while poopnoclip == true do 
            wait()
        game:GetService("Players").LocalPlayer.Character.Head.Touched:connect(function(obj)
        if obj ~= workspace.Terrain and poopnoclip == true  then 
        obj.CanCollide = false
        wait(30)
        obj.CanCollide = true
        end
        end) 
    end
    
end}) 

