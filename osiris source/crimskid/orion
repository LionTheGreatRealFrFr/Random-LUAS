GripPosition:AddDropdown({
	Name = "Presets",
	Default = "0",
	Options = {"High Sniper", "Invisible", "Sky Snipe"},
	Callback = function(v)
	    if v == "High Sniper" then 
	   local balls = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
	   
	   balls.GripPos = Vector3.new(0,-150,0)
	                       MoonGod.prompt('Crimskid', 'Must have a sniper equiped (shoot a couple times for the gripos to work)', 5)

	   
	    end 
   	    if v == "Invisible" then 
	   	   local balls = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
	   
	   balls.GripPos = Vector3.new(6,6,6)
	    end 
   	    if v == "Sky Snipe" then 
	    local balls = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
	   
	   balls.GripPos = Vector3.new(0,-50,0)
	    end 
   
   
end}) 


