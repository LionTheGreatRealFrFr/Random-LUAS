getgenv().FlingRadius = 5
getgenv().VelocityFlingPower = 200 
AntiAims:AddToggle({
	Name = "Velocity Fling",
	Default = false,
	Callback = function(v)
local oldposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame 
          _G.desync = v 
          
          
          while _G.desync == true do 
          task.wait()

          local x  = math.random(-getgenv().VelocityFlingPower,getgenv().VelocityFlingPower)
          local y  = math.random(-getgenv().VelocityFlingPower,getgenv().VelocityFlingPower)
            local z  = math.random(-getgenv().VelocityFlingPower,getgenv().VelocityFlingPower)
            local a = math.random(-getgenv().FlingRadius,getgenv().FlingRadius) 
            local b = math.random(-getgenv().FlingRadius,getgenv().FlingRadius)
          local c = math.random(-getgenv().FlingRadius,getgenv().FlingRadius)

                            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(x,y,z) 
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldposition * CFrame.new(a,b,c)
                            
                            
end 
 


end}) 



