getgenv().AimlockEnabled = false 
getgenv().Resolverr = false
getgenv().Part = "UpperTorso"
getgenv().Pred = 0.12
local IsLocking = false
getgenv().Airshot = true 
getgenv().DropdownPart = "UpperTorso"

        local CC = game:GetService"Workspace".CurrentCamera
        local LocalMouse = game.Players.LocalPlayer:GetMouse()
        
        
        function cockandballtorture()
            local closestPlayer
            local shortestDistance = math.huge
 
            for i, v in pairs(game.Players:GetPlayers()) do
                if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health ~= 0 and v.Character:FindFirstChild("LowerTorso") then
                    local pos = CC:WorldToViewportPoint(v.Character.PrimaryPart.Position)
                    local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(LocalMouse.X, LocalMouse.Y)).magnitude
                    if magnitude < shortestDistance then
                        closestPlayer = v
                        shortestDistance = magnitude
                    end
                end
            end
            return closestPlayer
        end
        

game:GetService("UserInputService").InputBegan:Connect(function(inp)
if inp.UserInputType == Enum.UserInputType.MouseButton2 then
aim = true
        lol =  cockandballtorture()
end



end) 

game:GetService("UserInputService").InputEnded:Connect(function(inp)
if inp.UserInputType == Enum.UserInputType.MouseButton2 then
aim = false
end
end)

game:GetService("RunService").heartbeat:Connect(function()
if Plr.Character.Humanoid.Jump == true and Plr.Character.Humanoid.FloorMaterial == Enum.Material.Air and getgenv().Airshot == true  then
                getgenv().Part = "RightFoot"
else
                    getgenv().Part = getgenv().DropdownPart
end 
end)


game:GetService("RunService").heartbeat:Connect(function()
                       

if aim then
    if getgenv().Resolverr == false and getgenv().AimlockEnabled == true 
 then 
game.Workspace.CurrentCamera.CFrame = CFrame.new(game.Workspace.CurrentCamera.CFrame.Position, lol.Character[getgenv().Part].Position + (lol.Character[getgenv().Part].Velocity*getgenv().Pred))
end 

if getgenv().Resolverr == true and getgenv().AimlockEnabled == true then
    game.Workspace.CurrentCamera.CFrame = CFrame.new(game.Workspace.CurrentCamera.CFrame.Position, lol.Character[getgenv().Part].Position)
end 
end 
end)

