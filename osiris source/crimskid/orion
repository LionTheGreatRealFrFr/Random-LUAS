Misc:AddToggle({
	Name = "Auto Stomp",
	Default = false,
	Callback = function(v)
	    _G.autostomp = v
while _G.autostomp == true do
    task.wait()
local args = {
    [1] = "Stomp"
}

game:GetService("ReplicatedStorage").MainRemote:FireServer(unpack(args))
end
end}) 


Misc:AddToggle({
	Name = "Fast Shooting DB",
	Default = false,
	Callback = function(v)
	   
getgenv().FastDB = v

game:GetService("RunService").RenderStepped:Connect(function()   
   if game:GetService("Players").LocalPlayer.Information.Armory["[Double Barrel SG]"].Ammo.Normal.Value == 1 and getgenv().FastDB == true then 
local args = {
    [1] = "Reload"
}

game:GetService("ReplicatedStorage").MainRemote:FireServer(unpack(args))

    end 
end) 


end}) 

